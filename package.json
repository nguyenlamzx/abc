{
  "name": "babel-7-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Example TypeScript project built on top of new Babel 7 features",
  "keywords": [
    "babel",
    "example",
    "boilerplate",
    "webpack",
    "typescript"
  ],
  "main": "./dist/index.js",
  "repository": "git@github.com:damassi/babel-7-typescript-example.git",
  "author": "Christopher Pappas <damassi.pappas@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.js --progress --profile --color --display-error-details --display-cached",
    "clean": "rimraf dist .cache-loader",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./configs/webpack.config.js",
    "prepublishOnly": "yarn build",
    "test:watch": "yarn test -o --watch",
    "lint": "tslint \"src/**/*.ts\""
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.32",
    "@babel/core": "^7.0.0-beta.32",
    "@babel/node": "^7.0.0-beta.32",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/preset-typescript": "^7.0.0-beta.32",
    "@babel/register": "^7.0.0-beta.32",
    "app-root-path": "^2.2.1",
    "autoprefixer": "^9.5.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-console": "0.2.1",
    "babel-plugin-module-resolver": "^3.0.0",
    "cache-loader": "^3.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "fork-ts-checker-notifier-webpack-plugin": "^1.0.0",
    "fork-ts-checker-webpack-plugin": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.12.1",
    "postcss-load-config": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-plugin": "^1.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "7.1.0",
    "style-loader": "^0.23.1",
    "thread-loader": "^2.1.2",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.5",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-fail-plugin": "^2.0.0"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.0",
    "popper.js": "^1.15.0"
  },
  "browserslist": [
    "defaults",
    "IE 11"
  ]
}
